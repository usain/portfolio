/*========================================Text Styles ===========================*/

.wrapper{
    width: 960px;
    margin: 0 auto;
}

header {
	width: 100%;
	height: 50px;
	background: white;
    position: fixed;
    z-index: 1000;

	h1 {
	    font-size: 14px;
	    display: inline-block;
	    padding: 10px;
	    line-height: 10px;
	    float: left;
	}
}

nav {
    height: 50px;
    float: right;
    margin: 0 20px 0 0;
    position: relative;

	    li {
		    float: right;

		    a {
				padding: 24px 20px 0 20px;
				display: inline-block;
				font-size: 14px;
                position: relative;
                z-index: 15;
		    }

            a:hover {
                color: #333333;
            }
		}

    .navSelect {
        background-color: #F65857;
        margin-top: 5px;
        border-radius: 6px;
        position: absolute;
        z-index: 10;
    }    
}

/*========================================Scene One===========================*/

.sceneOne {
    background: url('../img/landscape.png') repeat-x bottom;
    position: relative;

