   /*----- @font-face fonts -----*/



/* ==========================================================================
   Base styles: opinionated defaults
   ========================================================================== */

html,
button,
input,
select,
textarea {
    color: #222;
}

body {
    font-size: 1em;
    line-height: 1.4;
    // background-color: #E6E6E6;
    background-color: #C5F4FF;
    color: #333333;
    font-family: 'Open Sans', sans-serif;
}

/*
 * Remove text-shadow in selection highlight: h5bp.com/i
 * These selection declarations have to be separate.
 * Customize the background color to match your design.
 */

::-moz-selection {
    background: #b3d4fc;
    text-shadow: none;
}

::selection {
    background: #b3d4fc;
    text-shadow: none;
}

/*
 * A better looking default horizontal rule
 */

hr {
    display: block;
    height: 1px;
    border: 0;
    border-top: 1px solid #ccc;
    margin: 1em 0;
    padding: 0;
}

/*
 * Remove the gap between images and the bottom of their containers: h5bp.com/i/440
 */

img {
    vertical-align: middle;
}

/*
 * Remove default fieldset styles.
 */

fieldset {
    border: 0;
    margin: 0;
    padding: 0;
}

/*
 * Allow only vertical resizing of textareas.
 */

textarea {
    resize: vertical;
}

/* ==========================================================================
   Chrome Frame prompt
   ========================================================================== */

.chromeframe {
    margin: 0.2em 0;
    background: #ccc;
    color: #000;
    padding: 0.2em 0;
}

/* ==========================================================================
   Author's custom styles

   ========================================================================== */
h1,h2,h3,h4,h5,h6 {
    font-family: 'Ubuntu', sans-serif;
    margin: 10px;
}

a {
    text-decoration: none;
    color: #333333;
}

a:hover {
    // text-shadow: 1px 1px 0 #000;
    color: #F65857;
}

p {
    color: #353535;
    font-weight: bold;
    font-size: 18px;
    margin: 0;
}

ul {
	padding: 0;
	margin: 0;
}

li {    
    list-style: none;
    margin: 0;
    padding: 0;
}

::selection {
	background: #FF6839; /* Safari */
}

::-moz-selection {
	background: #FF6839; /* Firefox */
}

/*========================================Text Styles ===========================*/

.wrapper{
    width: 960px;
    margin: 0 auto;
}

header {
	width: 100%;
	height: 50px;
	background: white;
    position: fixed;
    z-index: 1000;

	h1 {
	    font-size: 14px;
	    display: inline-block;
	    padding: 10px;
	    line-height: 10px;
	    float: left;
	}
}

nav {
    height: 50px;
    float: right;
    margin: 0 20px 0 0;
    position: relative;

	    li {
		    float: right;

		    a {
				padding: 24px 20px 0 20px;
				display: inline-block;
				font-size: 14px;
                position: relative;
                z-index: 15;
		    }

            a:hover {
                color: #333333;
            }
		}

    .navSelect {
        background-color: #F65857;
        margin-top: 5px;
        border-radius: 6px;
        position: absolute;
        z-index: 10;
    }    
}

/*========================================Scene One===========================*/

.sceneOne {
    background: url('../img/landscape.png') repeat-x bottom;
    position: relative;

    .wrapper {
        position: relative;

	   img {
	   	margin: 0 auto;
	   }

        .bushOne{
            position: absolute;
            top: 370px;
            left: 40px;
            z-index: 10;
        }

        .bushTwo{
            position: absolute;
            top: 350px;
            left: 300px;
            z-index: 15;
        }

        .bushThree{
            position: absolute;
            top: 275px;
            left: 99px;
            z-index: 5;
        }

        .bushFour{
            position: absolute;
            top: 375px;
            left: 800px;
            z-index: 20;
        }

        .codeGuyOne {
            position: absolute;
            top: 150px;
            left: 700px;
            z-index: 20;
        }
        .codeGuyShadow {
            width: 100px;
            height: 10px;
            border-radius: 6px;
            background-color: grey;
            position: absolute;
            top: 435px;
            left: 735px;
            z-index: 10;
        }

        .codeGuyTwo {
            position: absolute;
            top: 230px;
            left: 210px;
            z-index: 10;
        }
    }

}

/*======================================== Work scene two ===========================*/

.sceneTwo {
    background: url('../img/volcanoDrape.png') repeat-x, #B3B3B3;
    padding-top: 80px;

	.work{
		margin: 60px auto;

        h2:hover {
            color: #333333;
        }

        a:hover {
            color: #333333;
        }
		
		.showcaseOne {
			// float: left;
            display: inline-block;
            cursor: pointer;
			height: 205px;
			background: url('../img/lavaOne.png'), #FF6839;
			width: 440px;
			border: solid 10px #F65857;
			border-radius: 6px;
			margin: 10px;
			position: relative;
			// display: table;

            img {
                width: 100%;
                position: absolute;
                top: 0;
                left: 0;
                z-index: 1;
            }

            h2 {
                display: table; 
                vertical-align: middle; 
                text-align: center;
                position: relative;
                z-index: 5;
                margin: 80px auto;
            }
		}

		.showcaseTwo {
			// float: left;
            cursor: pointer;
            display: inline-block;
			height: 205px;
			background: url('../img/lavaTwo.png'), #FF6839;
			width: 195px;
			border: solid 10px #F65857;
			border-radius: 6px;
			margin: 10px;
			position: relative;
			// display: table;

            h2 {
                display: table; 
                vertical-align: middle; 
                text-align: center;
                position: relative;
                z-index: 5;
                margin: 80px auto;
            }
		}

        .dropdown {
            display: block;
            background: #FF6839;
            padding-right: 10px;
            width: 850px;
            border: solid 10px #F65857;
            border-radius: 6px;
            margin-left: 40px;
            position: relative;
            float: left;
            display: none;

            img {
                width: 300px;
                height: 300px;
                display: block;
                margin: 10px 20px 10px 10px;
                float: left;
            }

            .button {
                position: absolute;
                padding: 10px 20px;
                border-radius: 6px;
                margin: 20px;
                background-color: #F65857;
                right: 0;
                bottom: 0;
            }
            
            ul {
                float: left;

                li {
                    padding: 5px;
                    float: left;
                    font-weight: bold;
                }
            }
        }

	}

}

/*======================================== Skills scene three ===========================*/

.sceneThree {
    background: url('../img/cavernDrape.png') repeat-x, #999999;
    padding-top: 80px;

    .skills {
        margin: 60px auto;
    }

    h3 {
        font-size: 48px;
        text-align: center;
        margin: 0 0 30px -10px;
    }

    .diamondsContainer{
        float: left;
        margin: 0 0 20px 10px;
        width: 530px;
        // border-right: solid #727272 6px;
        padding: 0 0 30px 0;

        .diamond {
            float: left;
            width: 100px;
            height: 100px;
            border-radius: 6px;
            background-color: #50FFB4;
            z-index: 10;
            position: relative;
            display: table;
            cursor: pointer;
            -webkit-transform: rotate(45deg);  
            -moz-transform: rotate(45deg);  
             -ms-transform: rotate(45deg);  
              -o-transform: rotate(45deg);   
                 transform: rotate(45deg);     

                .centerP {
                    font-weight: bold;
                    display: table-cell; 
                    vertical-align: middle; 
                    text-align: center;
                    -webkit-transform: rotate(-45deg);  
                    -moz-transform: rotate(-45deg);  
                     -ms-transform: rotate(-45deg);  
                      -o-transform: rotate(-45deg);   
                         transform: rotate(-45deg);                    
                }

                .shine {
                    position: absolute;
                    top: -10px;
                    left: 20px;
                    width: 25px;
                    height: 25px;
                    background-color: white;
                    border-radius: 6px;
                    opacity: 0.5;
                }

                .skillsign {
                    font-size: 24px;
                }

            }

        .shadow {
            float: left;
            width: 100px;
            height: 100px;
            border-radius: 6px;
            background-color: grey;
            z-index: 5;
            position: absolute;
            display: table;
            -webkit-transform: rotate(45deg);  
            -moz-transform: rotate(45deg);  
             -ms-transform: rotate(45deg);  
              -o-transform: rotate(45deg);   
                 transform: rotate(45deg);  
        }

        .slatOne {
            width: 457px;
            margin: 0 auto;
            position: relative;

            .diamond {
                margin-left: 60px;
            }

            .diamond:first-child {
                margin: 0;
            }

            .one {
                top: 10px;
            }

            .two {
                top: 10px;
                left: 160px;
            }

            .three {
                top: 10px;
                left: 320px;
            }

        }

        .slatTwo {
            width: 310px;
            margin: 0 auto;
            position: relative;

            .diamond {
                margin-left: 70px;
            }

            .diamond:first-child {
                margin: 0;
            }

            .four {
                top: 10px;
            }

            .five {
                top: 10px;
                left: 170px;
            }

        }
        
        .slatThree {
            width: 140px;
            margin: 0 auto;
            position: relative;

            .diamond {
                margin-left: 60px;
            }

            .diamond:first-child {
                margin: 0;
            }

            .six {
                top: 10px;
                // left: 170px;
            }

        }

    }

    .cvContainer {
        float: left;
        margin: 20px 10px 20px 0;
        width: 400px;
        // border-left: solid #727272 3px;

        p {
            display: inline-block;
            text-align: center;
            font-size: 24px;
            height: auto;
            font-weight: bold;
            margin-left: 30px;
        }

        p:first-child {
            margin-top: 30px;
        }

        .cvButton {
            display: inline-block;
            font-weight: bold;
            // width: 300px;
            // height: 60px;
            background-color: #F65857;
            border-radius: 6px;
            font-size: 24px;
            padding: 18px 59px;
            margin: 0 auto;
            margin: 24px 0 0 60px;
        }
    }
}

/*======================================== contact scene four ===========================*/

.sceneFour {
    background: url('../img/subterraneanDrape.png') repeat-x, #727272;
    padding-top: 80px;

    .contact {
        margin: 60px auto;
    }

    .messageContainer {
        float: left;
        margin: 20px 0 20px 10px;
        width: 467px;
    }

    form {
        float: left;

        .contact-field {
            margin-bottom: 20px;
        }

        input {
            width: 450px;
            height: 35px;
            line-height: 32px;
            border-radius: 6px;
            float: right;
            border: none;
            padding: 1px 0 0 10px;
        }

        ::-webkit-input-placeholder {
            font-weight: bold;
            font-size: 24px;
            color: #333333;
        }
        :-moz-input-placeholder {
            font-weight: bold;
            font-size: 24px;
            color: #333333;   
        }

        textarea {
            width: 450px;
            resize: none;
            border-radius: 6px;
            float: right;
            border: none;
            padding: 4px 0 0 10px;
        }

        #send {
            background-color: #F65857;
            font-weight: bold;
            font-size: 24px;
            float: right;
            width: auto;
            padding: 19px 41px;
            height: auto;
            line-height: 0;
            border-radius: 6px;
            border: none;
        }

    }
}


/*========================================Footer===========================*/

footer{
    width: 100%;
    height: 84px;
    background: black;

        .wrapper{
            position: relative;

            .backToTop {
                position: absolute;
                border-radius: 100px;
                width: 100px;
                height: 100px;
                background-color: black;
                top: -40px;
                left: 400px;

                img{
                    display: block;
                    margin: 20px auto;
                }
            }

        }

        p {
            display: inline-block;
            margin: 32px 20px;
            color: white;
            font-size: 12px;
        }

        .socialMedia {
            width: 64px;
            height: 64px;
            background-color: #F65857;
            border-radius: 6px;
            float: right;
            margin: 10px 5px;
        }
}

/* ==========================================================================
   CSS Animations

   ========================================================================== */

    .codeGuyTwo {   //the red character 
        -webkit-animation-name: codeGuyTwo;
        -webkit-animation-duration: 10s;
        -webkit-animation-timing-function: ease-out;
        -webkit-animation-iteration-count: 1;   
        -webkit-animation-direction: normal;
        -webkit-animation-delay: 1s;
        -webkit-animation-play-state: running;
        -webkit-animation-fill-mode: forwards;
    }

    @-webkit-keyframes codeGuyTwo {
    
        0% {
            width: 230px;
        }
        
        50% {
            width: 100px;
        }

        100% {
            width: 230px;
        }
    }


/* ==========================================================================
   Helper classes
   ========================================================================== */

/*
 * Image replacement
 */

.ir {
    background-color: transparent;
    border: 0;
    overflow: hidden;
    /* IE 6/7 fallback */
    *text-indent: -9999px;
}

.ir:before {
    content: "";
    display: block;
    width: 0;
    height: 150%;
}

/*
 * Hide from both screenreaders and browsers: h5bp.com/u
 */

.hidden {
    display: none !important;
    visibility: hidden;
}

/*
 * Hide only visually, but have it available for screenreaders: h5bp.com/v
 */

.visuallyhidden {
    border: 0;
    clip: rect(0 0 0 0);
    height: 1px;
    margin: -1px;
    overflow: hidden;
    padding: 0;
    position: absolute;
    width: 1px;
}

/*
 * Extends the .visuallyhidden class to allow the element to be focusable
 * when navigated to via the keyboard: h5bp.com/p
 */

.visuallyhidden.focusable:active,
.visuallyhidden.focusable:focus {
    clip: auto;
    height: auto;
    margin: 0;
    overflow: visible;
    position: static;
    width: auto;
}

/*
 * Hide visually and from screenreaders, but maintain layout
 */

.invisible {
    visibility: hidden;
}

/*
 * Clearfix: contain floats
 *
 * For modern browsers
 * 1. The space content is one way to avoid an Opera bug when the
 *    `contenteditable` attribute is included anywhere else in the document.
 *    Otherwise it causes space to appear at the top and bottom of elements
 *    that receive the `clearfix` class.
 * 2. The use of `table` rather than `block` is only necessary if using
 *    `:before` to contain the top-margins of child elements.
 */

.clearfix:before,
.clearfix:after {
    content: " "; /* 1 */
    display: table; /* 2 */
}

.clearfix:after {
    clear: both;
}

/*
 * For IE 6/7 only
 * Include this rule to trigger hasLayout and contain floats.
 */

.clearfix {
    *zoom: 1;
}

/* ==========================================================================
   EXAMPLE Media Queries for Responsive Design.
   Theses examples override the primary ('mobile first') styles.
   Modify as content requires.
   ========================================================================== */

@media only screen and (min-width: 35em) {
    /* Style adjustments for viewports that meet the condition */
}

@media print,
       (-o-min-device-pixel-ratio: 5/4),
       (-webkit-min-device-pixel-ratio: 1.25),
       (min-resolution: 120dpi) {
    /* Style adjustments for high resolution devices */
}

/* ==========================================================================
   Print styles.
   Inlined to avoid required HTTP connection: h5bp.com/r
   ========================================================================== */

@media print {
    * {
        background: transparent !important;
        color: #000 !important; /* Black prints faster: h5bp.com/s */
        box-shadow: none !important;
        text-shadow: none !important;
    }

    a,
    a:visited {
        text-decoration: underline;
    }

    a[href]:after {
        content: " (" attr(href) ")";
    }

    abbr[title]:after {
        content: " (" attr(title) ")";
    }

    /*
     * Don't show links for images, or javascript/internal links
     */

    .ir a:after,
    a[href^="javascript:"]:after,
    a[href^="#"]:after {
        content: "";
    }

    pre,
    blockquote {
        border: 1px solid #999;
        page-break-inside: avoid;
    }

    thead {
        display: table-header-group; /* h5bp.com/t */
    }

    tr,
    img {
        page-break-inside: avoid;
    }

    img {
        max-width: 100% !important;
    }

    @page {
        margin: 0.5cm;
    }

    p,
    h2,
    h3 {
        orphans: 3;
        widows: 3;
    }

    h2,
    h3 {
        page-break-after: avoid;
    }
}
