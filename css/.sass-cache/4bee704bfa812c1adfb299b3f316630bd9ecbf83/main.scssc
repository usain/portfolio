/*======================================== contact scene four ===========================*/

.sceneFour {
    min-width: 1024px;
    background: url('../img/subterraneanDrape.png') repeat-x, #727272;
    padding-top: 80px;

    .contact {
        margin: 60px auto;
        max-width: 960px;
    }

    .message {
        margin: 0 auto;/*10/960*100*/
        width: 940px;/*467/960*100*/
        position: relative;

        h2, p {
            text-align: center;
        }
    }

    .contactContainer {
        width: 640px;
        margin: 50px auto;
    }

    .contactDetails {
        width: 220px;
        background: silver;
        border-radius: 6px;
        padding: 20px;
        float: left;
        margin-right: 20px;

        h3 {
            text-align: center;
            margin: 0 0 20px;
        }

        .slat {
            margin-bottom: 5px;
            p {
                float: left;
                line-height: 32px;
                font-size: 14px;
                margin-left: 10px;
            }
            
            span {
                float: left;
                display: block;
                width: 32px;
                height: 32px;
                background: url(../img/contactImg.svg);
            }

            span.email {
                background-position: -64px 0;
            }

            span.location{
                background-position: -128px 0;
            }
        }
    }

    .contact-form {
        float: left;
        width: 360px;/*460/960*100*/
        max-width: 500px;
        // margin: 50px auto;

        .contact-field {
            margin-bottom: 20px;
        }

        input {
            width: 350px;/*460/460*100*/
            height: 35px;
            line-height: 32px;
            border-radius: 6px;
            float: right;
            border: none;
            padding: 1px 0 0 10px;
        }

        ::-webkit-input-placeholder {
            font-weight: bold;
            font-size: 24px;
            color: #333333;
        }
        :-moz-input-placeholder {
            font-weight: bold;
            font-size: 24px;
            color: #333333;   
        }

        textarea {
            width: 350px;/*450/460*100*/
            resize: none;
            border-radius: 6px;
            float: right;
            border: none;
            padding: 4px 0 0 10px;
            height: 200px;
        }

        #send {
            background-color: #F65857;
            font-weight: bold;
            font-size: 24px;
            float: right;
            width: auto;
            padding: 19px 41px;
            height: auto;
            line-height: 0;
            border-radius: 6px;
            border: none;
            margin-top: 5px;
        }

        #send:hover {
            border-bottom: 5px solid #F72B2B;
            margin-top: 0px;
            // margin-bottom: 5px;
        }

    }
}


/*========================================Footer===========================*/

footer{
    min-width: 1024px;
    width: 100%;
    
    background: black;

        .wrapper{
            position: relative;

            .backToTop {
                position: absolute;
                border-radius: 25px;
