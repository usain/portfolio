        p {
            display: inline-block;
            margin: 32px 20px;
            color: white;
            font-size: 12px;
            font-weight: lighter;
        }

        .socialMedia {
            width: 32px;
            height: 32px;
            // background-color: #F65857;
            border-radius: 6px;
            float: right;
            margin: 24px 5px;
        }

        .socialMedia:hover {
            border-bottom: 3px solid #F72B2B;
            margin-top: 21px;
        }

        .facebook {
            background: url(../img/facebook.svg) #F65857;
            background-size: cover;
        }

        .twitter {
            background: url(../img/twitter.svg) #F65857;
            background-size: cover;
        }

        .linkedin {
            background: url(../img/linkedin.svg) #F65857;
            background-size: cover;
        }
}

