/* ==========================================================================
   Base styles: opinionated defaults
   ========================================================================== */

html,
button,
input,
select,
textarea {
    color: #222;
}

body {
    font-size: 1em;
    line-height: 1.4;
    // background-color: #E6E6E6;
    background-color: #C5F4FF;
    min-width: 300px;
    color: #333333;
    font-family: 'Open Sans', sans-serif;
}

/*
 * Remove text-shadow in selection highlight: h5bp.com/i
 * These selection declarations have to be separate.
 * Customize the background color to match your design.
 */

::-moz-selection {
    background: #b3d4fc;
    text-shadow: none;
}

::selection {
    background: #b3d4fc;
    text-shadow: none;
}

/*
 * A better looking default horizontal rule
 */

hr {
    display: block;
    height: 1px;
    border: 0;
    border-top: 1px solid #ccc;
    margin: 1em 0;
    padding: 0;
}

/*
 * Remove the gap between images and the bottom of their containers: h5bp.com/i/440
 */

img {
    vertical-align: middle;
}

/*
 * Remove default fieldset styles.
 */

fieldset {
    border: 0;
    margin: 0;
    padding: 0;
}

/*
 * Allow only vertical resizing of textareas.
 */

textarea {
    resize: vertical;
}

/* ==========================================================================
   Chrome Frame prompt
   ========================================================================== */

.chromeframe {
    margin: 0.2em 0;
    background: #ccc;
    color: #000;
    padding: 0.2em 0;
}

/* ==========================================================================
   Author's custom styles

   ========================================================================== */
h1,h2,h3,h4,h5,h6 {
    font-family: 'Ubuntu', sans-serif;
    // margin: 1.6em ;/**/
}

a {
    text-decoration: none;
    color: #333333;
}

a:hover {
    // text-shadow: 1px 1px 0 #000;
    color: #F65857;
}

p {
    color: #353535;
    font-weight: bold;
    font-size: 1.125em;/*18/16*/
    margin: 0;
}

ul {
	padding: 0;
	margin: 0;
}

li {    
    list-style: none;
    margin: 0;
    padding: 0;
}

::selection {
	background: gold; /* Safari */
}

::-moz-selection {
	background: gold; /* Firefox */
}

/*========================================Text Styles ===========================*/

.wrapper{
    // width: 93.75%;/*960/1024*100*/
    // max-width: 1400px;
    width: 960px;
    margin: 0 auto;
}

header {
	width: 100%;
	height: 3.125em;/*50/16*/
	background: white;
    position: fixed;
    z-index: 1000;

	h1 {
	    font-size: 0.875em;/*14/16*/
	    display: inline-block;
	    padding: 10px;
	    line-height: 0.625em;/*10/16*/
	    float: left;
	}
}

nav {
    height: 3.125em;/*50/16*/
    // min-width: 300px;
    float: right;
    margin: 0 20px 0 0;
    position: relative;

	    li {
		    float: right;

		    a {
				padding: 24px 20px 0 20px;
				display: inline-block;
				font-size: 14px;
                position: relative;
                z-index: 15;
		    }

            a:hover {
                color: #333333;
            }
		}

    .navSelect {
        background-color: #F65857;
        margin-top: 5px;
        border-radius: 6px;
        position: absolute;
        z-index: 10;
    }    
}

/*========================================Scene One===========================*/

.sceneOne {
    background-image: url('../img/grassBG.svg'), url('../img/cityscapeBG.svg');
    background-position: bottom, center bottom;
    background-repeat: repeat-x, no-repeat;
    position: relative;

    .wrapper.overflow {
        position: relative;
        overflow: hidden;
        height: 600px;
        margin-top: 50px;

	   img {
        position: absolute;
	   	margin: 0 auto;
	   }

       img:first-child {
        top: 370px;
        right: 0;
       }

       img:nth-child(2) {
        top: 460px;
        z-index: 20;
        left: 275px;
       }

       img:nth-child(3) {
        top: 385px;
       }

        // .codeGuyOne {
        //     position: absolute;
        //     top: 150px;
        //     left: 72.91666666667%;/*700/960*100*/
        //     z-index: 20;
        //     width: 200px;
        //     height: 250px;
        //     background: url('../img/codeGuy1.svg') no-repeat;
        //     background-size: 90%;
        // }

        .speechBubble {
            width: 390px;
            // height: 260px;
            background-color: white;
            border-radius: 20px;
            padding: 20px;
            position: absolute;
            top: 200px;
            left: 35%;
            border-top: silver 6px solid;

            p {
                text-align: center;
            }
        }

        .codeGuyTwo {
            position: absolute;
            top: 350px;
            left: 180px;
            z-index: 10;
            width: 200px;
            height: 200px;
            background: url('../img/codeGuy2.svg');
            background-repeat: no-repeat;
            background-size: 100%;
        }
    }

}

    .codeGuyTwo {   //the red character 
        -webkit-animation-name: codeGuyTwo;
        -webkit-animation-duration: 5s;
        -webkit-animation-timing-function: ease-out;
        -webkit-animation-iteration-count: infinite;   
        -webkit-animation-direction: normal;
        -webkit-animation-delay: 1s;
        -webkit-animation-play-state: running;
        -webkit-animation-fill-mode: forwards;
    }

    @-webkit-keyframes codeGuyTwo {
    
        0% {
            // width: 200px;
        }
        
        50% {
            top: 390px;
            left: 190px;
            width: 150px;
        }

        100% {
            width: 200px;
        }
    }

/*======================================== Skills scene two ===========================*/

.sceneTwo {
    background: url('../img/volcanoDrape.svg') repeat-x, #B3B3B3;
    padding-top: 80px;

       .wrapper {
        max-width: 960px;
    }

    .skills {
        margin: 60px auto;
    }

    h3 {
        text-align: center;
        margin: 0 0 30px -10px;
    }

    .pipeSpace {
        width: 470px;

        .pipeMain {
            width: 80px;
            height: 280px;
            background-color: #999999;

            .badge {
                width: 60px;
                height: 60px;
                display: block;
                margin: 20px 10px;
            }

            .badge:nth-child(2) {
                margin: 30px 10px;
            }
        }
    }

    .percent {
        width: 310px;
        height: 40px;
        margin: 10px 10px;
    }

    .pipeLeft {
        float: left;
        padding: 90px 10px 90px 0;
        background-image: url(../img/pipeTop.svg), url(../img/pipeBottom.svg);
        background-repeat: no-repeat;
        background-position: top right, bottom right;

            .html {
                background-color:#f46622;
            }
            .css {
                background-color:#339dd5;
            }
            .jquery {
                background-color:#111a2a;
            }
        
        .pipeMain {
            float: right;
        }

            .pipe {
                width: 320px;
                height: 60px;
                background-color: #727272;
                margin: 20px 0 10px 0;
                float: right;
            }

    }

    .pipeRight {
        float: right;
        padding: 90px 0 90px 10px;
        background-image: url(../img/pipeTop.svg), url(../img/pipeBottom.svg);
        background-repeat: no-repeat;
        background-position: top left, bottom left;

        .percent {
            margin: 10px 10px 0 0;
        }

            .misc {
                background-color:#f15034;
            }
            .php {
                background-color:#5969a4;
            }
            .wordpress {
                background-color:#1c769f;
            }

        .pipeMain {
            float: left;
        }

            .pipe {
                width: 320px;
                height: 60px;
                background-color: #727272;
                margin: 20px 0 10px 0;
                float: left;
            }

    }

    .cvContainer {
        clear: both;
        margin: 20px 10px;/*10/960*100*/
        width: 100%;/*400/1400*100*/
        min-width: 400px;
        // border-left: solid #727272 3px;

        p {
            // display: inline-block;
            text-align: center;
            // font-size: 24px;
            height: auto;
            // font-weight: bold;
            margin-left: 30px;
        }

        p:first-child {
            margin-top: 30px;
        }

        .cvButton {
            display: inline-block;
            font-weight: bold;
            // width: 205px;
            background-color: #F65857;
            border-radius: 6px;
            font-size: 24px;
            padding: 18px 59px;
            margin: 10px 0 0 33%;/*60/960*100*/
        }

        .cvButton:hover {
            color: #333333;
            margin-bottom: -5px;
            border-bottom: 5px solid #F72B2B;
        }
    }

}

/*======================================== Work scene three ===========================*/

.sceneThree {
    background: url('../img/cavernDrape.png') repeat-x, #999999;
    padding-top: 80px;
    
        .work{
        max-width: 1400px;
        margin: 60px auto;
        // padding-left: 7%;

        h2:hover {
            color: #333333;
        }

        a:hover {
            color: #333333;
        }
        
        .showcaseOne {
            // float: left;
            display: inline-block;
            cursor: pointer;
            height: 205px;
            background: url('../img/lavaOne.svg'), #FF6839;
            width: 45.83333333333%;/*440/960*100*/
            min-width: 440px;
            border: solid 10px #F65857;
            border-radius: 6px;
            margin: 10px;
            position: relative;
            // display: table;

            img {
                width: 100%;
                position: absolute;
                top: 0;
                left: 0;
                z-index: 1;
            }

            h2 {
                display: table; 
                vertical-align: middle; 
                text-align: center;
                position: relative;
                z-index: 5;
                margin: 80px auto;
            }
        }

        .showcaseTwo {
            // float: left;
            cursor: pointer;
            display: inline-block;
            height: 205px;
            background: url('../img/lavaTwo.svg'), #FF6839;
            width: 20.3125%;/*195/960*100*/
            min-width: 195px;
            border: solid 10px #F65857;
            border-radius: 6px;
            margin: 10px;
            position: relative;
            // display: table;

            h2 {
                display: table; 
                vertical-align: middle; 
                text-align: center;
                position: relative;
                z-index: 5;
                margin: 80px auto;
            }
        }

        .dropdown {
            display: block;
            background: #FF6839;
            padding-right: 10px;
            width: 88.54166666667%;/*850/960*100*/
            max-width: 1200px;
            border: solid 10px #F65857;
            border-radius: 6px;
            margin-left: 4.166666666667%;/*40/960*100*/
            position: relative;
            float: left;
            display: none;

            img {
                width: 35.29411764706%;/*300/850*100*/
                height: 300px;
                max-width: 300px;
                display: block;
                margin: 10px 2.352941176471% 10px 1.176470588235%;/*20/850*100 | 10/850*100*/
                float: left;
            }

            .button {
                position: absolute;
                padding: 10px 20px;
                border-radius: 6px;
                margin: 20px;
                background-color: #F65857;
                right: 0;
                bottom: 0;
            }

            .button:hover {
                border-bottom: 5px solid #F72B2B;
            }
            
            ul {
                float: left;

                li {
                    padding: 5px;
                    float: left;
                    font-weight: bold;
                }
            }
        }

    }
 
}

/*======================================== contact scene four ===========================*/

.sceneFour {
    background: url('../img/subterraneanDrape.png') repeat-x, #727272;
    padding-top: 80px;

    .contact {
        margin: 60px auto;
        max-width: 960px;
    }

    .messageContainer {
        margin: 0 auto;/*10/960*100*/
        width: 940px;/*467/960*100*/
        position: relative;

        h2, p {
            text-align: center;
        }
    }

    form {
        width: 47.91666666667%;/*460/960*100*/
        max-width: 500px;
        margin: 50px auto;

        .contact-field {
            margin-bottom: 20px;
        }

        input {
            width: 97.82608695652%;/*450/460*100*/
            height: 35px;
            line-height: 32px;
            border-radius: 6px;
            float: right;
            border: none;
            padding: 1px 0 0 10px;
        }

        ::-webkit-input-placeholder {
            font-weight: bold;
            font-size: 24px;
            color: #333333;
        }
        :-moz-input-placeholder {
            font-weight: bold;
            font-size: 24px;
            color: #333333;   
        }

        textarea {
            width: 97.82608695652%;/*450/460*100*/
            resize: none;
            border-radius: 6px;
            float: right;
            border: none;
            padding: 4px 0 0 10px;
        }

        #send {
            background-color: #F65857;
            font-weight: bold;
            font-size: 24px;
            float: right;
            width: auto;
            padding: 19px 41px;
            height: auto;
            line-height: 0;
            border-radius: 6px;
            border: none;
        }

        #send:hover {
            border-bottom: 5px solid #F72B2B;
            margin-bottom: -5px;
        }

    }
}


/*========================================Footer===========================*/

footer{
    width: 100%;
    
    background: black;

        .wrapper{
            position: relative;

            .backToTop {
                position: absolute;
                border-radius: 25px;
                width: 100px;
                height: 60px;
                background-color: black;
                top: -40px;
                left: 47%;/*400px*/

                img{
                    display: block;
                    margin: 20px auto;
                }
            }

        }

        p {
            display: inline-block;
            margin: 32px 20px;
            color: white;
            font-size: 12px;
            font-weight: lighter;
        }

        .socialMedia {
            width: 32px;
            height: 32px;
            // background-color: #F65857;
            border-radius: 6px;
            float: right;
            margin: 24px 5px;
        }

        .facebook {
            background: url(../img/facebook.svg) #F65857;
            background-size: cover;
        }

        .twitter {
            background: url(../img/twitter.svg) #F65857;
            background-size: cover;
        }

        .linkedin {
            background: url(../img/linkedin.svg) #F65857;
            background-size: cover;
        }
}

