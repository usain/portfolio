                    width: 93%;/*435/960*100*/
                    margin: 5px auto;

                    img {
                        width: 100%;
                        position: absolute;
                        top: 0;
                        left: 0;
                        z-index: 1;
                    }

                    h2 {
                        display: table; 
                        vertical-align: middle; 
                        text-align: center;
                        position: relative;
                        z-index: 5;
                        margin: 80px auto;
                    }
                }

                .showcaseTwo {
                    // float: left;
                    cursor: pointer;
                    display: inline-block;
                    height: 205px;
                    background: url('../img/lavaTwo.svg'), #FF6839;
                    width: 93%;/*435/960*100*/
                    border: solid 10px #F65857;
                    border-radius: 6px;
                    margin: 5px auto;
                    position: relative;
                    // display: table;

                    h2 {
                        display: table; 
                        vertical-align: middle; 
                        text-align: center;
                        position: relative;
                        z-index: 5;
                        margin: 80px auto;
                    }
                }

            .dropdown {
                width: 89%;/*850/960*100*/
                margin-left: 0%;/*40/960*100*/

                img {
                    width: 100%;/*300/850*100*/
                    height: 300px;
                    max-width: 300px;
                    display: block;
                    margin: 10px auto;/*20/850*100 | 10/850*100*/
                    float: none;
                }

                
            }

        }
     
    }//end of seneThree -----------------------------------

    .sceneFour {

        .contact {
            margin: 60px auto;
            max-width: 960px;
        }

        .contactContainer {
            width: 93.75%;/*640/960*100*/
            margin: 50px auto;
        }

        .contactDetails {
            width: 93.75%;/*220/960*100*/
            background: silver;
            border-radius: 6px;
            padding: 10px 10px;
            float: none;
            margin-right: 0;


            .slat {
                margin: 0 auto 5px auto;

                p {
                    float: right;
                    line-height: 32px;
                    font-size: 12px;
                    margin-left: 10px;
                }
                
                span {
                    float: left;
                    display: block;
                    width: 32px;
                    height: 32px;
                }
            }
        }

        .contact-form {
            margin-top: 10px;
            float: none;
            width: 93.75%;/*360/640*100*/

            input {
                width: 90%;/*350/360*100*/
            }

            textarea {
                width: 90%;/*350/360*100*/
            }

        }
    }//end of sceneFour ----------------------------

    footer{
    width: 100%;
    
    background: black;

        .wrapper{
            position: relative;

            .backToTop {
                left: 35%;/*400px*/
            }

        }

        p {
            display: block;
            margin: 32px auto;
            text-align: center;
        }

        .socialMedia {
            width: 64px;
